{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "max-len": ["error", { "code": 120 }],
    "quotes": ["error", "single"],
    "camelcase": "off",
    "no-console": "off"
  }
}
